{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Vary",
          "value": "x-vercel-ip-country"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "CD"
        }
      ],
      "destination": "/fr/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/fr", "destination": "/fr/index.html" },
    { "source": "/add-transaction", "destination": "/add-transaction.html" },
    { "source": "/fr/add-transaction", "destination": "/fr/add-transaction.html" },
    { "source": "/admin-kyc", "destination": "/admin-kyc.html" },
    { "source": "/fr/admin-kyc", "destination": "/fr/admin-kyc.html" },
    { "source": "/article-detail", "destination": "/article-detail.html" },
    { "source": "/fr/article-detail", "destination": "/fr/article-detail.html" },
    { "source": "/chatbot", "destination": "/chatbot.html" },
    { "source": "/fr/chatbot", "destination": "/fr/chatbot.html" },
    { "source": "/community", "destination": "/community.html" },
    { "source": "/fr/community", "destination": "/fr/community.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/fr/dashboard", "destination": "/fr/dashboard.html" },
    { "source": "/edit-transaction", "destination": "/edit-transaction.html" },
    { "source": "/fr/edit-transaction", "destination": "/fr/edit-transaction.html" },
